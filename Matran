#include<bits/stdc++.h>
using namespace std;
void nhapmang(int a[99][99], int m, int n){
	for(int i=0; i<m; i++){
		for(int j=0; j<n; j++){
			cout <<"a["<<i<<","<<j<<"]=";
			cin>>a[i][j];
		}
	}
}
void xuatmang(int a[99][99], int m, int n){
	for(int i=0; i<m; i++){
		for(int j=0; j<n; j++){
			cout<<a[i][j]<<" ";
		}
		cout <<"\n";
	}
}
int tong(int a[99][99], int m, int n){
	int s=0;
	for(int i=0; i<m; i++){
		for(int j=0; j<n; j++){
			s = s+ a[i][j];
		}
	}
	cout<<"\ntong la: "<<s;
}
int tongchan(int a[99][99], int m, int n){
	int tc=0;
	for(int i=0; i<m; i++){
		for(int j=0; j<n; j++){
			if(a[i][j] % 2==0){
				tc += a[i][j];
			}
		}
	}
	cout<<"\ntong chan la: "<<tc;
}
int tongnt(int a[99][99], int m, int n){
	int tnt=0;
	for(int i=0; i<m; i++){
		for(int j=0; j<n; j++){
			for(int k>1;  )
		}
	}
}
int main(){
	int a[99][99];
	int m,n;
	cin>>m>>n;
	nhapmang(a,m,n);
	xuatmang(a,m,n);
	tong(a,m,n);
	tongchan(a,m,n);
	return 0;
}
